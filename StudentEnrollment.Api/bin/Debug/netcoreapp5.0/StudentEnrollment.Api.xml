             <summary>
             Creates a Course.
             </summary>
             <returns>A newly created TodoItem</returns>
             <remarks>
             Sample request:
            
              
                 POST /Add Course  
                 {
                    "name": "MAT 00",
                    "title": "string",
                    "abbreviation": "MAT",
                    "section": "01",
                    "courseNumber": "string",
                    "instructorName": "User",
                    "department": "Mathematics",
                    "credits": 0,
                    "startTime": "9:00am",
                    "endTime": "9:00am",
                    "remainingSlots": 0,
                    "capacity": 0
                 }
            
            </remarks>
            <response code="201">Returns the newly created item</response>
            <response code="400">If the item is null</response>
        </member>
